@model IEnumerable<CarInsurance.Models.Insuree>

<h2>All Quotes</h2>

<table class="table">
    <tr>
        <th>First Name</th>
        <th>Last Name</th>
        <th>Email Address</th>
        <th>Quote</th>
    </tr>
    @foreach (var item in Model)
    {
        <tr>
            <td>@item.FirstName</td>
            <td>@item.LastName</td>
            <td>@item.EmailAddress</td>
            <td>@String.Format("{0:C}", item.Quote)</td>
        </tr>
    }
</table>
